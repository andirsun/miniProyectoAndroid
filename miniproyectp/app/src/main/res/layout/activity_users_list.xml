<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Controladores.UsersListActivity"
        android:background="@color/colorBackGround">

    <ListView
            android:layout_width="381dp"
            android:layout_height="646dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:id="@+id/listUsers"
            android:clickable="true" app:layout_constraintHorizontal_bias="0.533">
        <Button
                android:text="Mis Reservas"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginTop="40dp"
                app:layout_constraintTop_toBottomOf="@+id/emailProfile"
                android:id="@+id/volver3"
                android:textColor="@color/colorText"
                android:textColorHint="@color/colorPrimary"
                android:background="@android:color/holo_red_light"
                android:ems="10" app:layout_constraintHorizontal_bias="0.497"/>
    </ListView>
</android.support.constraint.ConstraintLayout>